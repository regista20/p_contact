<%= form_for(@contact) do |f| %>
  <%= render 'shared/error_messages' %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name%>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email%>
  </div>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title%>
  </div>
  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_area :content%>
  </div>
  <div class="actions">
    <%= f.submit "確認" %>
  </div>
<% end %>
